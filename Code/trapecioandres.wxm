/* [wxMaxima: input   start ] */
IteracionFuncional(g, ti, wi, wi10, h, numiter,tolerancia):=block(
    for i:1 thru numiter do(
        wi1: wi + (h/2)*(g(ti,wi)+g(ti+h,wi10)),
        if abs(wi1-wi10)<tolerancia then return(wi1)
        else wi10:wi1
    ),
    return (wi1)
);
/* [wxMaxima: input   end   ] */

